        {
            "name": "Horst Oeko",
            "email": "horst@volloeko.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "horstoeko\\zugferd\\": "src",
            "horstoeko\\zugferd\\rsm\\": "src/entities/rsm",
